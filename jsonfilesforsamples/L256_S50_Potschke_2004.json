        "MATERIALS": {
            "Matrix": {
                "MatrixComponent": {
                    "ChemicalName": "polycarbonate",
                    "ManufacturerName": "GE Europe",
                    "TradeName": "Lexan 121"
                },
                "MatrixProcessing": {
                    "ChooseParameter": {
                        "Drying-Evaporation": {
                            "Description": "Prior to mixing the materials were dried for about 16 h at 80 8C in a vacuum oven.",
                            "Temperature": {
