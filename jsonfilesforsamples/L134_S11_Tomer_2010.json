        "PROCESSING": {
            "SolutionProcessing": [
                {
                    "ChooseParameter": {
                        "Solvent": "12 g of purified BT powder were suspended in a solution of 90 ml ethanol, 10 ml distilled water and 0.6 g of 3-glycidoxypropyl)trimethoxysilane"
                    }
                },
                {
                    "ChooseParameter": [
                        {
                            "Mixing": {
                                "Description": "The mixture was stirred for 24 h",
                                "MixingMethod": "stirring"
                            }
                        },
                        {
                            "Centrifugation": "Used"
                        },
                        {
                            "Heating": {
                                "Description": "subsequently centrifuged for 10 min and finally the precipitated modified powder was dried at 120 deg C for 6 h.",
                                "Temperature": {
                                    "value": "120",
                                    "unit": "Celsius"
                                },
                                "Time": {
                                    "value": "6",
                                    "unit": "hours"
                                }
                            }
                        },
                        {
                            "Mixing": {
                                "Description": "The composites were prepared by adding the fillers into the epoxy resin.  The dispersion of the particles was aided by high shear mixing.",
                                "MixingMethod": "high shear mixing"
                            }
                        },
                        {
                            "Mixing": {
                                "Description": "Sonication of the suspension.",
                                "MixingMethod": "ultra-sonication"
                            }
                        },
                        {
                            "Mixing": {
                                "Description": "The suspensions were charged with appropriate amounts of crosslinker, 2-ethyl-4-methylimidazole (Curing Agent Imicure EMI-24) and were degassed under vacuum to remove any trapped air.",
                                "MixingMethod": "stirring",
                                "ChemicalUsed": "2-ethyl-4-methylimidazole"
                            }
                        },
                        {
                            "Heating": {
                                "Description": "Fills 100 um thick ere obtained by casting the solutions between teflon plates and placing them on a hot plate to accelerate the curing process.  The samples were cured at 60 deg C for 3 h and postcured at 180 deg C.",
                                "Temperature": {
                                    "value": "60",
                                    "unit": "Celsius"
                                },
                                "Time": {
                                    "value": "3",
                                    "unit": "hours"
                                }
                            }
                        },
                        {
                            "Heating": {
                                "Description": "post-cured",
