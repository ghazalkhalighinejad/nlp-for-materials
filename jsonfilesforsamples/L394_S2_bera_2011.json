        "CHARACTERIZATION": {
            "Scanning_Electron_Microscopy": {
                "EquipmentUsed": "Scanning Electron Microscope JEOL JSM-6460",
                "Description": "The characterization of polystyrene nanocomposites was done using scanning electron microscope JEOL JSM-6460 at magnifications from 103 to 2 \u00d7 106 at 25 kV. For SEM morphological investigations, the surfaces were coated with gold using BAL-TEC SCD 005 instrument. This \u201cSputter coater\u201d procedure was applied at 30 mA, under vacuum 10\u22126 Torr for 90 s, in order to avoid charging under the electron beam. The brightness and contrast of the digitised SEM micrographs were adjusted using standard software.",
                "AcceleratingVoltage": {
                    "value": "25.0",
                    "unit": "kV"
                },
                "EmissionCurrent": {
                    "description": "This \u201cSputter coater\u201d procedure was applied at 30 mA, under vacuum 10\u22126 Torr for 90 s, in order to avoid charging under the electron beam.",
                    "value": "30.0",
                    "unit": "mA"
                },
                "ExposureTime": {
                    "value": "90.0",
                    "unit": "seconds"
                }
            },
            "Differential_Scanning_Calorimetry": {
                "Equipment": "Q20 TA Instruments equipment",
                "Description": "DSC measurements were carried out using Q20 TA Instruments equipment in nitrogen atmosphere (50 cm3/min), in standard pans without lid. In order to erase the previous thermal history, samples with masses of about 3 mg were kept as melts at 250 \u00b0C for 10 min and cooled down to 60 \u00b0C. Finally, the samples were heated up to 140 \u00b0C with a scan rate of 10 \u00b0C/min. The glass transition temperatures were determined on the basis of the second heating."
            },
            "Fourier_Transform_Infrared_Spectroscopy": {
                "Equipment": "Thermo Nicolet Nexus 670 FTIR spectrometer",
                "Description": "Measurements were performed at room temperature using a Thermo Nicolet Nexus 670 FTIR spectrometer. Single beam spectra of the samples were obtained after averaging 40 scans between 4000 cm\u22121 and 500 cm\u22121 with a resolution of 2 cm\u22121, in a transmittance mode."
            },
            "Thermogravimetric_Analysis": {
                "Equipment": "TA Instruments\u2019 SDT Q600 TG/DSC thermal analyzer",
                "Description": "Thermal stability of polystyrene and polystyrene/silica nanocomposites films was determined using TA Instruments\u2019 SDT Q600 TG/DSC thermal analyzer. Simultaneous TG/DSC measurements were carried out up to 500 \u00b0C in nitrogen gas carrier (100 cm3/min) employing an open ceramic crucible with a corresponding empty referent crucible and sample masses of about 3 mg. The heating rate was 20 \u00b0C/min."
            }
